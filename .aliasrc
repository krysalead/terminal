#!/usr/bin/env bash
# all aliases for command shorthand
alias ll='ls -la'
alias q='exit'
alias c='clear'
alias home='cd ~'
alias root='cd /'
alias work='cd ~/Projects/git_clones'
alias lab='cd ~/Projects/lab'
alias o=open
alias ..='cd ..'
alias branches='git branch --sort=-committerdate'
alias master='git checkout master'
alias development='git checkout development'
alias production='git checkout production'
alias release='git checkout release'
alias previous='git checkout @{-1}'
alias sha1sum="shasum"
eval "$(thefuck --alias)"alias python=python3.7
alias python='/usr/bin/python3.7'
alias kub='kubectl'
alias ns='kubens'
alias ctx='kubectx'
alias files='find * -type f | fzf > selected'